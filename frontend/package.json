  "scripts": {
    "start:dev": "vite",
    "start:build": "NODE_ENV=production tsc && vite build",
    "cb": "vite build",
    "start:preview": "vite preview --port 3334",
    "serve": "NODE_ENV=production tsc && vite build && vite preview --port 3334",
    "serve:dev": "cross-env NODE_ENV=production DOMAIN_OVERRIDE=1 tsc && vite build && vite preview --port 3334",
    "typecheck": "tsc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint . --ext .ts,.tsx --cache",
    "lint:stylelint": "stylelint '**/*.css' --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "toc": "i18next-resources-for-ts toc -i ./public/locales/en -o @types/resources.ts"
  },
